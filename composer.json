{
    "name": "creativestyle/utilities",
    "description": "Static method utilities collection with high reusability",
    "keywords": ["string", "array", "utilities"],
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Sobalski",
            "email": "filip.sobalski@creativestyle.pl",
            "homepage": "http://creativestyle.pl"
        }
    ],
    "require": {
        "php": ">=8",
        "paragonie/random-lib": "^2.0",
        "symfony/options-resolver": "^2 | ^3 | ^4 | ^5 | ^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload":     {
        "psr-4": {
            "Creativestyle\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creativestyle\\Utilities\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8"
        }
    }
}
